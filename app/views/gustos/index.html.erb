<p id="notice"><%= notice %></p>

<h1>Gustos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gustos.each do |gusto| %>
      <tr>
        <td><%= gusto.Nombre %></td>
        <td><%= link_to 'Show', gusto %></td>
        <td><%= link_to 'Edit', edit_gusto_path(gusto) %></td>
        <td><%= link_to 'Destroy', gusto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gusto', new_gusto_path %>
